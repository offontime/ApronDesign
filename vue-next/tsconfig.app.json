{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "example/**/*", "example/**/*.ts", "example/**/*.vue"],
  "exclude": ["example/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["example/*"]
    }
  }
}
